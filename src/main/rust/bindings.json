[
    {
        "path": "android/input"
    }, {
        "path": "android/log"
    }, {
        "deps": [ "jni" ], 
        "match": [ "bitmap", "stdint", "_types", "stddef" ], 
        "path": "android/bitmap"
    }, {
        "deps": [ "jni", "android::native_window" ], 
        "match": [ "native_window_jni" ], 
        "path": "android/native_window_jni"
    }, {
        "path": "android/native_window"
    }, {
        "path": "jni", 
        "remove_fnptr_opts": true
    }, {
        "deps": [ "lua::raw" ], 
        "match": [ "luajit" ], 
        "path": "luajit", 
        "root": "../jni/lua"
    }, {
        "path": "redirect_stderr",
        "root": "../jni"
    }
]
